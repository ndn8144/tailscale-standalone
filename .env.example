# Tailscale Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Tailscale auth key
# Get this from: https://login.tailscale.com/admin/settings/keys
TAILSCALE_AUTH_KEY=tskey-auth-your-key-here

# Optional: Tailscale OAuth settings (for API access)
TS_OAUTH_CLIENT_ID=your-oauth-client-id
TS_OAUTH_CLIENT_SECRET=your-oauth-client-secret
TS_TAILNET=your-tailnet-name

# Optional: Build settings
BUILD_OUTPUT_DIR=builds
TEMP_DIR=temp
LOG_LEVEL=INFO
